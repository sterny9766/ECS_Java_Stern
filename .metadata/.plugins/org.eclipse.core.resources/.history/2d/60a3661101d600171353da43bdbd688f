
public class UsingVariabless 
{
	public static void main( String[] args )
	{
		int first = 113;
        double E = 2.71828;
        String word = "Computer Science";
        System.out.println("This is room # " + first);
        System.out.println("e is close to " + E);
        System.out.println("I am learning a bit about " + word);
			
	}
}
